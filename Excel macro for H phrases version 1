Function GetHphraseText(inputH As String) As String
    Dim hPhrases As Object
    Set hPhrases = CreateObject("Scripting.Dictionary")
    
    ' LÃ¤gg till H-fraser och deras beskrivningar
    hPhrases.Add "H200", "Unstable explosive"
    hPhrases.Add "H201", "Explosive; mass explosion hazard"
    hPhrases.Add "H202", "Explosive; severe projection hazard"
    hPhrases.Add "H203", "Explosive; fire, blast, or projection hazard"
    hPhrases.Add "H204", "Fire or projection hazard"
    hPhrases.Add "H205", "May mass explode in fire"
    hPhrases.Add "H220", "Extremely flammable gas"
    hPhrases.Add "H221", "Flammable gas"
    hPhrases.Add "H222", "Extremely flammable aerosol"
    hPhrases.Add "H223", "Flammable aerosol"
    hPhrases.Add "H224", "Extremely flammable liquid and vapor"
    hPhrases.Add "H225", "Highly flammable liquid and vapor"
    hPhrases.Add "H226", "Flammable liquid and vapor"
    hPhrases.Add "H228", "Flammable solid"
    hPhrases.Add "H229", "Pressurized container: may burst if heated"
    hPhrases.Add "H230", "May react explosively even in the absence of air"
    hPhrases.Add "H231", "May react explosively even in the absence of air at elevated pressure and/or temperature"
    hPhrases.Add "H240", "Heating may cause an explosion"
    hPhrases.Add "H241", "Heating may cause a fire or explosion"
    hPhrases.Add "H242", "Heating may cause a fire"
    hPhrases.Add "H250", "Catches fire spontaneously if exposed to air"
    hPhrases.Add "H251", "Self-heating; may catch fire"
    hPhrases.Add "H252", "Self-heating in large quantities; may catch fire"
    hPhrases.Add "H260", "In contact with water releases flammable gases which may ignite spontaneously"
    hPhrases.Add "H261", "In contact with water releases flammable gases"
    hPhrases.Add "H270", "May cause or intensify fire; oxidizer"
    hPhrases.Add "H271", "May cause fire or explosion; strong oxidizer"
    hPhrases.Add "H272", "May intensify fire; oxidizer"
    hPhrases.Add "H280", "Contains gas under pressure; may explode if heated"
    hPhrases.Add "H281", "Contains refrigerated gas; may cause cryogenic burns or injury"
    hPhrases.Add "H290", "May be corrosive to metals"
    hPhrases.Add "H300", "Fatal if swallowed"
    hPhrases.Add "H301", "Toxic if swallowed"
    hPhrases.Add "H302", "Harmful if swallowed"
    hPhrases.Add "H304", "May be fatal if swallowed and enters airways"
    hPhrases.Add "H310", "Fatal in contact with skin"
    hPhrases.Add "H311", "Toxic in contact with skin"
    hPhrases.Add "H312", "Harmful in contact with skin"
    hPhrases.Add "H314", "Causes severe skin burns and eye damage"
    hPhrases.Add "H315", "Causes skin irritation"
    hPhrases.Add "H317", "May cause an allergic skin reaction"
    hPhrases.Add "H318", "Causes serious eye damage"
    hPhrases.Add "H319", "Causes serious eye irritation"
    hPhrases.Add "H330", "Fatal if inhaled"
    hPhrases.Add "H331", "Toxic if inhaled"
    hPhrases.Add "H332", "Harmful if inhaled"
    hPhrases.Add "H334", "May cause allergy or asthma symptoms or breathing difficulties if inhaled"
    hPhrases.Add "H335", "May cause respiratory irritation"
    hPhrases.Add "H336", "May cause drowsiness or dizziness"
    hPhrases.Add "H340", "May cause genetic defects"
    hPhrases.Add "H341", "Suspected of causing genetic defects"
    hPhrases.Add "H350", "May cause cancer"
    hPhrases.Add "H351", "Suspected of causing cancer"
    hPhrases.Add "H360", "May damage fertility or the unborn child"
    hPhrases.Add "H361", "Suspected of damaging fertility or the unborn child"
    hPhrases.Add "H362", "May cause harm to breast-fed children"
    hPhrases.Add "H370", "Causes damage to organs"
    hPhrases.Add "H371", "May cause damage to organs"
    hPhrases.Add "H372", "Causes damage to organs through prolonged or repeated exposure"
    hPhrases.Add "H373", "May cause damage to organs through prolonged or repeated exposure"
    hPhrases.Add "H400", "Very toxic to aquatic life"
    hPhrases.Add "H410", "Very toxic to aquatic life with long-lasting effects"
    hPhrases.Add "H411", "Toxic to aquatic life with long-lasting effects"
    hPhrases.Add "H412", "Harmful to aquatic life with long-lasting effects"
    hPhrases.Add "H413", "May cause long-lasting harmful effects to aquatic life"
    hPhrases.Add "H420", "Harms public health and the environment by destroying ozone in the upper atmosphere"
    hPhrases.Add "H302+H312", "Harmful if swallowed or in contact with skin"
    hPhrases.Add "H302+H332", "Harmful if swallowed or if inhaled"
    hPhrases.Add "H310+H330", "Fatal in contact with skin or if inhaled"
    hPhrases.Add "H311+H331", "Toxic in contact with skin or if inhaled"
    hPhrases.Add "H314+H318", "Causes severe skin burns and eye damage"
    hPhrases.Add "H317+H334", "May cause an allergic skin reaction or allergy or asthma symptoms or breathing difficulties if inhaled"
    hPhrases.Add "H400+H410", "Very toxic to aquatic life. Very toxic to aquatic life with long-lasting effects"
    hPhrases.Add "", ""
    
    ' Kontrollera om input finns i listan
    If hPhrases.exists(inputH) Then
        GetHphraseText = inputH & ": " & hPhrases(inputH)
    Else
        GetHphraseText = "Invalid H-phrase"
    End If
End Function
